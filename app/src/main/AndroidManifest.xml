    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splashscreen"></activity>
        <activity android:name=".AcademicActivity" />
        <activity android:name=".AcademicCalender" />
        <activity android:name=".AboutusActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".UpdateProfile" />
        <activity android:name=".PasswordActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Developer" />
        <activity android:name=".profile" />
        <activity android:name=".About" />
        <activity android:name=".Dashboard" />
        <activity android:name=".FacultyDetails" />
        <activity android:name=".FacultyActivity" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".SubjectDetails" />
        <activity android:name=".SubjectActivity" />
        <activity android:name=".DayDetail" />
        <activity android:name=".WeekActivity" />
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <service
            android:name=".MyMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>